# flappy.kv
<FloatLayout>:
    Background:

<Background>:
    canvas.before:
        Rectangle:
            size: self.size
            pos: self.pos
            source: 'Background.jpg'
        Rectangle:
            size: 450, 100
            pos: self.pos[0], self.pos[1] + self.height - 100
            source: 'cloud.png'